{
    "globals": {
        "process":true,
        "__dirname":true
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent":["error",2],
        "quotes":[
            "error",
            "single"
        ],
        "semi":[
            "error",
            "never"
        ],
        "no-unused-vars":"off",
        "no-debugger":"off",
        "no-console":"off"
    }
}
